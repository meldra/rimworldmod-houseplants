<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="PlantBase">
    <defName>Plant_Zzplant</defName>
    <label>ZZ plant</label>
    <description>Also known as Zanzibar Gem or Zamiculcas zamiifolia, this is tuberous plant is beloved for its fronds of glossy leaflets.</description>
    <statBases>
      <MaxHitPoints>185</MaxHitPoints>
      <Beauty>20</Beauty>
      <Nutrition>0.01</Nutrition>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/Zzplant</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <altitudeLayer>Item</altitudeLayer>
    <selectable>true</selectable>
    <ingestible />
    <plant>
      <dieIfLeafless>true</dieIfLeafless>
      <harvestTag>Standard</harvestTag>
      <sowWork>900</sowWork>
      <sowTags>
        <li>Decorative</li>
      </sowTags>
      <growMinGlow>0.1</growMinGlow>
      <topWindExposure>0.1</topWindExposure>
      <growDays>14</growDays>
      <lifespanDaysPerGrowDays>28</lifespanDaysPerGrowDays>
      <visualSizeRange>1.25~1.25</visualSizeRange>
      <wildOrder>1</wildOrder>
      <purpose>Beauty</purpose>
    </plant>
  </ThingDef>
  
